@using SCORM1.Models.ViewModel
@model   AdminProfileViewModel

<div class="panel-primary">

    <br /><br /><br />
    <h1 class="title"> TÉRMINOS Y CONDICIONES</h1>
    <embed src="~/TermsandConditions/@Model.FileTerms" width="800" height="375">
    @using (Html.BeginForm("Validateterms", "AdminPoint", FormMethod.Post, new { @class = "form-horizontal", role = "form", enctype = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()
        <br /><br />
        <div class="col-md-offset-2 col-md-8">
            <br /><br /><br />
            @Html.CheckBoxFor(m => m.termsandconditions, htmlAttributes: new { @class = "", @style = "width: 50px;height: 30px; " })
            @Html.LabelFor(m => m.termsandconditions)
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-8">
                <br /><br />
                <center><button class="btn btn-primary" name="singin" type="submit">Continuar</button></center>
            </div>
        </div>
    }
</div>


